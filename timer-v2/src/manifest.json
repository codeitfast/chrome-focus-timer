{
  "manifest_version": 3,
  "name": "Tab View Counter",
  "version": "2.1.2",
  "permissions": ["storage", "tabs", "idle"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "index.html"
  },
  "icons": {
    "16": "timer-icon-2.png",
    "32": "timer-icon-2.png",
    "64": "timer-icon-2.png",
    "128": "timer-icon-2.png"
  }
}
