{
  "manifest_version": 3,
  "name": "Tab View Counter",
  "version": "1.0",
  "permissions": ["storage", "tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "dallE.png",
    "32": "dallE.png",
    "64": "dallE.png",
    "128": "dallE.png"
  }
}
